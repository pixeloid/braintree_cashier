{#
/**
 * @file subscription.html.twig
 * Default theme implementation to present Subscription data.
 *
 * This template is used when viewing Subscription pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_subscription()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('subscription') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
