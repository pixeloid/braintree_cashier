{
    "name": "drupal/braintree_cashier",
    "description": "Recurring billing managed by Braintree",
    "type": "drupal-module",
    "require": {
        "drupal/braintree_api": "^2.0",
        "moneyphp/money": "^3.0",
        "dompdf/dompdf": "^0.8.1"
    },
    "require-dev": {
        "vlucas/phpdotenv": "~2.4.0"
    },
    "suggest": {
        "symfony/intl": "Allows using Braintree Cashier on a host without PHP's intl extension. Requires PHP 7.1.3+ and choosing 'Force using the en locale' from Braintree Cashier settings."
    },
    "authors": [
        {
            "name": "Shaun Dychko",
            "email": "ShaunDychko@475828.no-reply.drupal.org"
        }
    ]
}
